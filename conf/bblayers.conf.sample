# POKY_BBLAYERS_CONF_VERSION is increased each time build/conf/bblayers.conf
# changes incompatibly
POKY_BBLAYERS_CONF_VERSION = "2"

BBPATH = "${TOPDIR}"
BBFILES ?= ""

BBLAYERS ?= " \
  ${HOME}/poky-styhead/meta \
  ${HOME}/poky-styhead/meta-poky \
  ${HOME}/poky-styhead/meta-openembedded/meta-oe \
  ${HOME}/poky-styhead/meta-openembedded/meta-networking \
  ${HOME}/poky-styhead/meta-openembedded/meta-python \
  ${HOME}/poky-styhead/meta-security \
  ${HOME}/poky-styhead/meta-qt6 \
  ${HOME}/bbb/meta-bbb \
"
